. $TRDIR/setup_mce_test.sh

ERROR_TYPE=hard-offline
ERROR_OFFSET=1
THP=true
EXPECTED_RETURN_CODE="START EXIT"
RACE_ITERATIONS=10

_control() {
	local pid=

	for i in $(seq 100) ; do
		$test_alloc_generic -n 1024 -o iterate_mapping -B thp &
		pid=$!
		sleep 0.3
		migratepages $pid 0 1
		migratepages $pid 1 0
		kill -9 $pid
	done
	set_return_code EXIT
}

_prepare() {
	echo "NOT PREPARED YET"
	return 1
	prepare_mce_test || return 1
}

_cleanup() {
	cleanup_mce_test
}

_check() {
	check_mce_test
}
