. $TRDIR/setup_mce_test.sh

EXPECTED_RETURN_CODE="START EXIT"

TESTFILE="$WDIR/testfile"

HUGETLB=100
HUGETLB_MOUNT=$WDIR/hugetlbfs
HUGETLB_FILE=$HUGETLB_MOUNT/testfile

_control() {
    local i=

	ps ax | grep test_alloc

    for i in $(seq 10) ; do
		echo i
		$test_alloc_generic -o madv_stress -B 0xffffffff -d work -e madv_hard
		$test_alloc_generic -o madv_stress -B 0xffffffff -d work -e madv_soft
		$test_alloc_generic -o madv_stress -B 0xffffffff -d work -e madv_hard -A
		$test_alloc_generic -o madv_stress -B 0xffffffff -d work -e madv_soft -A
    done
	ps ax | grep test_alloc
    set_return_code EXIT
}
